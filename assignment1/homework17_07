def is_prime(num):
    if num < 2:
        return False
    for i in range(2, int(num**0.5) + 1):
        if num % i == 0:
            return False
    return True

n = int(input("Enter a number: "))
position = 0
number = 1

while position < n:
    number += 1
    if is_prime(number):
        position += 1

print(number)   # This prints the nth prime
